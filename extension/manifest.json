{
  "manifest_version": 3,
  "name": "Simply Block! Website Blocker",
  "version": "1.0.0",
  "description": "Simply block websites that you don't want to visit anymore",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "webNavigation"
  ],
  "key": ""
}